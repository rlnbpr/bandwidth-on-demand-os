<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 2.1//EN"
       "http://tiles.apache.org/dtds/tiles-config_2_1.dtd">

<tiles-definitions>

  <definition name="default" template="/WEB-INF/layouts/default.jspx">
    <put-attribute name="menubar" value="/WEB-INF/views/menubar.jspx" />
    <put-attribute name="active_menu" value="none" cascade="true" />
  </definition>

  <definition name="noc" template="/WEB-INF/layouts/default.jspx">
    <put-attribute name="menubar" value="/WEB-INF/views/noc/menubar.jspx" />
    <put-attribute name="active_menu" value="none" cascade="true" />
  </definition>
  
  <definition name="manager" template="/WEB-INF/layouts/default.jspx">
    <put-attribute name="menubar" value="/WEB-INF/views/manager/menubar.jspx" />
    <put-attribute name="active_menu" value="none" cascade="true" />
  </definition>

</tiles-definitions>
