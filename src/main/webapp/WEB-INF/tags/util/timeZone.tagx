<jsp:root xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:fn="http://java.sun.com/jsp/jstl/functions"
    xmlns:spring="http://www.springframework.org/tags" xmlns:jsp="http://java.sun.com/JSP/Page" xmlns:bod="BodTags"  version="2.0">
    <jsp:output omit-xml-declaration="yes" />

    <bod:env property="defaultTimeZoneId" var="timeZone"/>
    <bod:env property="defaultTimeZoneOffset" var="timeZoneOffset"/>
    <span class="timezone">Time zone: ${timeZone} ${timeZoneOffset}</span>
</jsp:root>
