<html xmlns:spring="http://www.springframework.org/tags"
  xmlns:util="urn:jsptagdir:/WEB-INF/tags/util"
  xmlns:sec="http://www.springframework.org/security/tags"
  xmlns:jsp="http://java.sun.com/JSP/Page"
  xmlns:c="http://java.sun.com/jsp/jstl/core"
  xmlns:table="urn:jsptagdir:/WEB-INF/tags/table"
   xmlns:sform="http://www.springframework.org/tags/form">

<jsp:directive.page contentType="text/html;charset=UTF-8" />
<jsp:output omit-xml-declaration="yes" />
<spring:url value="/${baseReportIntervalUrl}/" var="reportIntervalUrl" />

<style>

#graph {
  fill: rgb(51, 51, 51);
}

svg {
  background-color: white;
  border: 1px solid black;
}

.axis path,
.axis line {
  fill: none;
  stroke: rgb(51, 51, 51);
  shape-rendering: crispEdges;
}

.axis line.tick {
  stroke: lightgray;
}

</style>

<section>
  <h2>Request graph</h2>
  <spring:url var="data_url" value="/noc/report/data.csv" />
  <spring:url var="spinner_url" value="/resources/images/spinner2.gif" />
  <div class="content" id="graph" data-url="${data_url}" data-spinner="${spinner_url}">
  <!--  -->
  </div>
</section>

</html>